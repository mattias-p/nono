digit = _{ '0'..'9' }
number = { digit+ }
clue = { number ~ ("," ~ number)* | "" }
clue_list = { clue ~ (";" ~ clue)* }
filled = { "#" }
crossed = { "X" | "x" }
undecided = { "." }
impossible = { "!" }
cell = { filled | crossed | undecided | impossible }
grid_line = ${ cell+ }
grid = { grid_line ~ (";" ~ grid_line)* }
puzzle = { "[" ~ clue_list ~ "|" ~ clue_list ~ ("|" ~ grid)? ~ "]" }
whitespace = _{ " " | "\r" | "\n" }
